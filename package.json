{
  "name": "securityportal-gui",
  "description": "NC State Security Portal",
  "version": "0.5.1",
  "license": "MIT",
  "homepage": "https://github.ncsu.edu/SAT/securityportal-gui/",
  "private": true,
  "engines": {
    "node": ">= 10.8.0",
    "npm": ">= 6.4"
  },
  "author": "Paul Kehle (pgkehle@ncsu.edu)",
  "contributors": [
    {
      "name": "Paul Kehle",
      "email": "pgkehle@ncsu.edu"
    },
    {
      "name": "William Howell",
      "email": "wshowell@ncsu.edu"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.ncsu.edu/SAT/securityportal-gui.git"
  },
  "bugs": {
    "url": "https://github.ncsu.edu/SAT/securityportal-gui/issues"
  },
  "scripts": {
    "deploy:clients:dev01": "ansible-playbook ansible/deploy.yml --limit app-dev01 -e \"build_type='dev:remote'\"",
    "deploy:clients:dev02": "ansible-playbook ansible/deploy.yml --limit app-dev02 -e \"build_type='dev:remote'\"",
    "deploy:clients:development": "ansible-playbook ansible/deploy.yml --limit sp-api-development -e \"build_type='dev:remote'\"",
    "deploy:clients:staging": "ansible-playbook ansible/deploy.yml --limit sp-api-staging -e \"build_type='prod'\"",
    "deploy:clients:production": "ansible-playbook ansible/deploy.yml --limit sp-api-production -e \"do_deploy=true build_type='prod'\"",
    "clean": "npm run clean:coverage && npm run clean:dist && npm run clean:modules",
    "clean:coverage": "rimraf -- ./coverage",
    "clean:dist": "rimraf -- ./dist",
    "clean:modules": "rimraf -- ./node_modules",
    "clean:reinstall": "npm run clean && npm install",
    "build": "npm run build:dev",
    "build:dev": "ng build --aot --build-optimizer",
    "build:dev:remote": "ng build --aot --build-optimizer --configuration=remote",
    "build:prod": "ng build --prod --aot --build-optimizer",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:dev:remote": "npm run build:dev:remote -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "test": "ng test",
    "lint": "ng lint securityportal-gui --type-check",
    "e2e": "ng e2e",
    "server": "ng serve --aot=false",
    "server:proxied": "ng serve --aot=false --configuration=proxied",
    "ci": "npm test && npm run e2e",
    "docs": "npm run typedoc -- --options typedoc.json --exclude \"**/*.spec.ts\" ./src/",
    "start": "npm run server",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "upgrade-interactive": "npm-upgrade check",
    "pretest": "npm run lint",
    "postversion": "git push && git push --tags",
    "postinstall": "node scripts/postinstall"
  },
  "dependencies": {
    "@agm/core": "^1.0.0-beta.5",
    "@angular/animations": "^7.0.3",
    "@angular/cdk": "^7.0.3",
    "@angular/common": "^7.0.3",
    "@angular/compiler": "^7.0.3",
    "@angular/core": "^7.0.3",
    "@angular/forms": "^7.0.3",
    "@angular/http": "^7.0.3",
    "@angular/material": "^7.0.3",
    "@angular/platform-browser": "^7.0.3",
    "@angular/platform-browser-dynamic": "^7.0.3",
    "@angular/router": "^7.0.3",
    "@ncstate/sat-popover": "^3.0.0",
    "@ngrx/effects": "^6.1.2",
    "@ngrx/router-store": "^6.1.2",
    "@ngrx/store": "^6.1.2",
    "@ngrx/store-devtools": "^6.1.2",
    "core-js": "^2.5.7",
    "date-fns": "^1.29.0",
    "hammerjs": "^2.0.8",
    "lodash-es": "^4.17.11",
    "ngx-date-fns": "^2.0.2",
    "normalize.css": "^8.0.0",
    "pluralize": "^7.0.0",
    "rxjs": "^6.3.3",
    "web-animations-js": "^2.3.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.4",
    "@angular/cli": "^7.0.4",
    "@angular/compiler-cli": "^7.0.3",
    "@angular/language-service": "^7.0.3",
    "@types/jasmine": "~2.8.11",
    "@types/jasminewd2": "~2.0.6",
    "@types/lodash-es": "~4.17.1",
    "@types/node": "~10.12.3",
    "@types/pluralize": "~0.0.29",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngrx-store-freeze": "^0.2.4",
    "npm-upgrade": "^1.4.1",
    "protractor": "~5.4.1",
    "rimraf": "^2.6.2",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "vrsource-tslint-rules": "^5.8.2"
  }
}
